{
    "extends": "tslint:latest",
    "rules": {
        "indent": [
            true,
            "spaces"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "object-literal-sort-keys": false,
        "class-name": true,
        "trailing-comma": false,
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "eofline": true,
        "jsdoc-format": true,
        "member-access": false,
        "no-console": false,
        "whitespace": [
            true,
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ],
        "variable-name": [
            "allow-leading-underscore"
        ],
        "curly": false,
        "no-reference": false,
        "no-var-requires": false,
        "no-trailing-whitespace": false,
        "ordered-imports": false,
        "max-line-length": [
            true,
            250
        ],
        "interface-name": [
            true,
            "never-prefix"
        ]
    },
    "rulesDirectory": []
}